# Primary backend API URL (FA-backend)
VITE_BASE_API=http://localhost:4001

# Google reCAPTCHA v3 site key
VITE_RECAPTCHA_SITE_KEY=

# Contact method: 'whatsapp' or 'telegram'
VITE_CONTACT_APP=telegram

# Contact phone number (without + prefix)
VITE_CONTACT_NUMBER=

# Telegram username (without @)
VITE_TELEGRAM_USER=

# Test backend URL (cuerpo-fit-tracking-backend-nestjs) for dual submission
# Used for parallel testing â€” the mirror submission is fire-and-forget
VITE_TEST_API=http://localhost:4002

# Enable dual submission to test backend ('true' to enable, anything else to disable)
# Only set to 'true' for local parallel testing or in deployed environments where you want mirror submissions
# Requires a redeploy to take effect (Vite replaces env vars at build time)
VITE_DUAL_SUBMIT=false